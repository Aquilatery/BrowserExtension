{
  "manifest_version": 2,
  "name": "Show browsing history",
  "version": "1.0",
  "permissions": [
    "tabs",
    "history",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_popup": "history.html",
    "default_icon": {
      "16": "icon16.png",
      "24": "icon24.png",
      "32": "icon32.png"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}